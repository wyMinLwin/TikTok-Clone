    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-tab-view": "^3.4.0",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.5",
    "expo-linear-gradient": "~12.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "tailwindcss": "^3.2.4"
  },
  "private": true
}
